{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Um clone para o tab news usado no curso.dev",
  "keywords": [],
  "author": "Felipe Oliveira",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "sh ./dev.sh",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "infra:up": "docker compose -f ./infra/compose.yaml up -d --force-recreate",
    "infra:stop": "docker compose -f ./infra/compose.yaml stop",
    "infra:down": "docker compose -f ./infra/compose.yaml down"
  },
  "dependencies": {
    "next": "^13.4.9",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@types/react": "18.2.38",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "typescript": "5.3.2"
  }
}